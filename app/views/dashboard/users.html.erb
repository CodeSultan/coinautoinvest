
<div class="wrapper">
  <!-- top navbar-->
  <header class="topnavbar-wrapper">
    <%= render partial: "layouts/partials/admin_topnavbar" %>
  </header>
  <!-- sidebar-->
  <aside class="aside">
    <%= render partial: "layouts/partials/admin_sidebar" %>
  </aside>
  <!-- offsidebar-->
  <aside class="offsidebar hide">
    <%= render partial: "layouts/partials/admin_offsidebar" %>
  </aside>
  <!-- Main section-->
  <section>
    <!-- Page content-->
    <div class="content-wrapper">
      <div>
        <h1 style="text-align: center">All Users</h1>
      </div>

      <table class="table table-bordered table-striped table-hover" role="grid">
        <thead style="border-radius: 5px">
        <tr>
          <th>Tier</th>
          <th>User ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Country Code</th>
          <th>Phone Number</th>
          <th>Verified</th>
          <th>Create Time</th>
          <th></th>
          <th></th>
        </tr>
        </thead>
        <tbody class="user-index">
          <%= render "userindex" %>
        </tbody>
      </table>

    </div>
  </section>
  <!-- Page footer-->
  <footer>
    <%= render partial: "layouts/partials/admin_footer" %>
  </footer>
</div>

<%= yield :body_area %>
